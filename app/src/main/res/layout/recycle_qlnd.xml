<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/recycle_QLND_hoten"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:height="50dp" />

        <TextView
            android:id="@+id/recycle_QLND_username"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:height="50dp" />

        <TextView
            android:id="@+id/recycle_QLND_password"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:height="50dp"/>


        <TextView
            android:id="@+id/recycle_QLND_sodienthoai"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:height="50dp" />

    </LinearLayout>



</LinearLayout>